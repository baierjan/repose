.\" vim: tw=72 fdm=marker cms=.\\"\ %s
.
.\" FRONT MATTER {{{
.Dd Feb 04, 2016
.Os
.Dt REPOSE-REMOVE 1
.
.Sh NAME
.Nm repose-remove
.Nd Remove matching repositories
.\" FRONT MATTER }}}
.
.\" SYNOPSIS {{{
.Sh SYNOPSIS
.Nm
.Fl h | \-help
.Nm
.Op Fl n | \-print
.Ar HOST Ns ...
.Li --
.Ar REPA Ns ...
.\" SYNOPSIS }}}
.
.\" DESCRIPTION {{{
.Sh DESCRIPTION
.Nm
removes specified package repositories.
.\" }}}
.
.\" OPTIONS {{{
.Sh OPTIONS
.
.Bl -tag -width "xx"
. It Fl h
Display usage instructions.
. It Fl \-help
Display this manual page.
. It Fl n Ns , Fl \-print
Write destructive operations to standard output,
do not actually perform them.
.El
.\" }}}
.
.\" OPERANDS {{{
.Sh OPERANDS
.
.Bl -tag -width "xx"
. It Ar HOST
Machine to operate on.
. It Ar REPA
Repository pattern (see
. Xr repose 1 Ns ).
Matching repositories will be removed.
.El
.\" }}}
.
.\" EXAMPLES {{{
.Sh EXAMPLES
.Bd -literal
$ repose remove root@fubar.example.org -- sled:::at,nv
.Ed
.Bd -literal
$ repose remove root@{fubar,snafu}.example.org -- sle-sdk sle-we
.Ed
.\" }}}
.
.\" SEE ALSO {{{
.Sh SEE ALSO
.Xr repose 1 ,
.Xr repose-add 1 ,
.Xr smrt 1 ,
.Xr ssh 1 ,
.Xr zypper 8 .
.\" }}}
.
.\" REPOSE {{{
.Sh REPOSE
.Nm
is part of
.Xr repose 1 .
.\" }}}
